pipeline{
    agent any
    
    stages{
        stage("One"){
            steps{
                timeout(unit: 'SECONDS', time: 5) {
                sh '''
                    END=100
                    for i in $(seq 1 $END); do
                    echo "Hello $i"
                    sleep 1
                    done
                sh '''
                }
            }   
        }
    }
}
